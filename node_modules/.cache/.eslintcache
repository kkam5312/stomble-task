rofileObj.name, password: \"\"})\n  }\n\n  return (\n    <div className=\"App\">\n      {(user.username !== \"\") ? (\n        <div className='welcome'>\n          <h2>Welcome, <span>{user.username}</span></h2>\n          <h3>Do you like.... hololive?</h3>\n          <button onClick={Logout}>Logout</button>\n        </div>  \n      ) : (\n        <div className='loginForms'>\n          \n          {((!register) ? (\n            <LoginForm responseGoogle={responseGoogle} Login={Login} error={error} handleRegister={handleRegister}/>\n          ) : (<RegisterForm responseGoogle={responseGoogle} Register={Register} error={error} handleRegister={handleRegister}/>))}\n        </div>\n      )}\n      \n      {console.log(register)}\n    </div>\n  );\n}\n\nexport default App;\n","/Users/justinkam/Desktop/stomble/login-form/src/reportWebVitals.js",[],"/Users/justinkam/Desktop/stomble/login-form/src/components/registerForm.jsx",["24"],"/Users/justinkam/Desktop/stomble/login-form/src/components/loginForm.jsx",["25"],{"ruleId":"26","severity":1,"message":"27","line":4,"column":8,"nodeType":"28","messageId":"29","endLine":4,"endColumn":19},{"ruleId":"30","severity":1,"message":"31","line":18,"column":25,"nodeType":"32","messageId":"33","endLine":18,"endColumn":27},{"ruleId":"30","severity":1,"message":"31","line":17,"column":25,"nodeType":"32","messageId":"33","endLine":17,"endColumn":27},"no-unused-vars","'GoogleLogin' is defined but never used.","Identifier","unusedVar","eqeqeq","Expected '!==' and instead saw '!='.","BinaryExpression","unexpected"]